Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", "x_", "]"}], ":=", 
   RowBox[{"Cos", "[", "x", "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"x0", "=", "0"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"x1", "=", "2.0"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"Nmax", "=", "5"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"eps", "=", "0.0001"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"N", "[", 
      RowBox[{
       RowBox[{"f", "[", "x0", "]"}], " ", 
       RowBox[{"f", "[", "x1", "]"}]}], "]"}], ">", "0"}], ",", 
    RowBox[{
    "Print", "[", 
     "\"\<\\nYour values do no satisfy the IVP so use new values.\>\"", "]"}],
     ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "<=", "Nmax"}], ",", 
       RowBox[{"i", "++"}], ",", 
       RowBox[{
        RowBox[{"m", "=", 
         FractionBox[
          RowBox[{"(", 
           RowBox[{"x0", "+", "x1"}], ")"}], "2"]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Abs", "[", 
            FractionBox[
             RowBox[{"x1", "-", "x0"}], "2"], "]"}], "<", "eps"}], ",", 
          RowBox[{"Return", "[", "m", "]"}], ",", " ", 
          RowBox[{
           RowBox[{"Print", "[", 
            RowBox[{
            "i", ",", " ", "\"\<th iteration value is : \>\"", ",", "m"}], 
            "]"}], ";", "\[IndentingNewLine]", 
           RowBox[{"Print", "[", 
            RowBox[{
            "\"\<Estimated error in \>\"", ",", " ", "i", ",", " ", 
             "\"\<th iteration is : \>\"", ",", " ", 
             FractionBox[
              RowBox[{"(", 
               RowBox[{"x1", "-", "x0"}], ")"}], "2"]}], "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"If", " ", "[", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"f", "[", "m", "]"}], "*", 
               RowBox[{"f", "[", "x1", "]"}]}], "<", "0"}], ",", 
             RowBox[{"x0", "=", "m"}], ",", 
             RowBox[{"x1", "=", "m"}]}], "]"}]}]}], "]"}]}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<\\nRoot is : \>\"", ",", "m"}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{
      "\"\<Estimated error in \>\"", ",", " ", "i", ",", " ", 
       "\"\<th iteration is : \>\"", ",", " ", 
       FractionBox[
        RowBox[{"(", 
         RowBox[{"x1", "-", "x0"}], ")"}], "2"]}], "]"}], ";"}]}], "]"}], 
  ";"}], "\[IndentingNewLine]", 
