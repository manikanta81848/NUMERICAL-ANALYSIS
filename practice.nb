Cell[BoxData[
 RowBox[{
  RowBox[{"LagrangePolynomial", "[", 
   RowBox[{"x0_", ",", "y0_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"xi", "=", "x0"}], ",", 
      RowBox[{"yi", "=", "y0"}], ",", "n", ",", "m", ",", "polynomial"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"n", "=", 
      RowBox[{"Length", "[", "xi", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"m", "=", 
      RowBox[{"Length", "[", "yi", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"m", "!=", "n"}], ",", 
       RowBox[{
        RowBox[{
        "Print", "[", 
         "\"\<\\nList of points and values are not of same size\>\"", "]"}], 
        ";", 
        RowBox[{"Return", "[", "]"}], ";"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "<=", "n"}], ",", 
       RowBox[{"i", "++"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"L", "[", 
          RowBox[{"i", ",", "x_"}], "]"}], "=", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            UnderoverscriptBox["\[Product]", 
             RowBox[{"j", "=", "1"}], 
             RowBox[{"i", "-", "1"}]], 
            FractionBox[
             RowBox[{"x", "-", 
              RowBox[{"xi", "[", 
               RowBox[{"[", "j", "]"}], "]"}]}], 
             RowBox[{
              RowBox[{"xi", "[", 
               RowBox[{"[", "i", "]"}], "]"}], "-", 
              RowBox[{"xi", "[", 
               RowBox[{"[", "j", "]"}], "]"}]}]]}], ")"}], 
          RowBox[{"(", 
           RowBox[{
            UnderoverscriptBox["\[Product]", 
             RowBox[{"j", "=", 
              RowBox[{"i", "+", "1"}]}], "n"], 
            FractionBox[
             RowBox[{"x", "-", 
              RowBox[{"xi", "[", 
               RowBox[{"[", "j", "]"}], "]"}]}], 
             RowBox[{
              RowBox[{"xi", "[", 
               RowBox[{"[", "i", "]"}], "]"}], "-", 
              RowBox[{"xi", "[", 
               RowBox[{"[", "j", "]"}], "]"}]}]]}], ")"}]}]}], ";"}]}], "]"}],
      ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"polynomial", "[", "x_", "]"}], "=", 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"k", "=", "1"}], "n"], 
       RowBox[{
        RowBox[{"L", "[", 
         RowBox[{"k", ",", "x"}], "]"}], "*", 
        RowBox[{"yi", "[", 
         RowBox[{"[", "k", "]"}], "]"}]}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", 
      RowBox[{"polynomial", "[", "x", "]"}], "]"}], ";"}]}], "]"}]}]], "Input",
